import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        try
        {
            int a=obj.nextInt();
            int b=obj.nextInt();
            System.out.print(a/b);
        }
        catch(InputMismatchException e)
        {
            System.out.print(e.getClass().getName());
        }
        catch(Exception e)
        {
            System.out.print(e);
        }
        
    }
}
